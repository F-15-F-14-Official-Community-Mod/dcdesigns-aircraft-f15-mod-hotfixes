<Template Name="DCDesigns_GT_Animation">    <DefaultTemplateParameters>        <ANIM_GUID />        <ANIM_LAG>0</ANIM_LAG>        <ANIM_LENGTH>100</ANIM_LENGTH>        <ANIM_WRAP>False</ANIM_WRAP>        <ANIM_DELTA>FALSE</ANIM_DELTA>        <ANIM_BLENDWEIGHT>1.0</ANIM_BLENDWEIGHT>        <ANIM_CODE />    </DefaultTemplateParameters>        <Animation Name = "#ANIM_NAME#" Guid = "#ANIM_GUID#" Length = "#ANIM_LENGTH#" Type = "Sim" TypeParam = "AutoPlay">        <Parameter>            <Code>#ANIM_CODE#</Code>            <Lag>#ANIM_LAG#</Lag>            <Wrap>#ANIM_WRAP#</Wrap>            <Delta>#ANIM_DELTA#</Delta>            <BlendWeight>#ANIM_BLENDWEIGHT#</BlendWeight>        </Parameter>    </Animation></Template><Template Name="DC_INTAKE">  <UseTemplate Name="DCDesigns_GT_Animation">      <ANIM_NAME>IntakesparentAction</ANIM_NAME>	  <ANIM_LENGTH>100</ANIM_LENGTH>      <ANIM_LAG>100</ANIM_LAG>      <ANIM_CODE>(A:GEAR HANDLE POSITION, bool) !             if{             (L:Intake Ramp L, bool)             if{ (A:INCIDENCE ALPHA, degrees) 5 &gt; if{ 100 } els{ 50 } } }             els{ 0 }</ANIM_CODE>  </UseTemplate></Template><Template Name="DC_RAMPSL">  <UseTemplate Name="DCDesigns_GT_Animation">      <ANIM_NAME>IntakeLAction</ANIM_NAME>	  <ANIM_LENGTH>100</ANIM_LENGTH>      <ANIM_LAG>100</ANIM_LAG>      <ANIM_CODE>(A:GEAR HANDLE POSITION, bool) (L:Covers Switch, bool) ! &amp;&amp;           if{		   (L:Intake Ramp L, bool) } if{ 100 } els{ 0 }</ANIM_CODE>  </UseTemplate></Template><Template Name="DC_RAMPSR">  <UseTemplate Name="DCDesigns_GT_Animation">      <ANIM_NAME>IntakeRAction</ANIM_NAME>	  <ANIM_LENGTH>100</ANIM_LENGTH>      <ANIM_LAG>100</ANIM_LAG>      <ANIM_CODE>(A:GEAR HANDLE POSITION, bool) (L:Covers Switch, bool) ! &amp;&amp;           if{		   (L:Intake Ramp R, bool) } if{ 100 } els{ 0 }</ANIM_CODE>  </UseTemplate></Template><Template Name="DC_Canopy">  <UseTemplate Name="DCDesigns_GT_Animation">      <ANIM_NAME>CanopyAction</ANIM_NAME>	  <ANIM_LENGTH>100</ANIM_LENGTH>      <ANIM_LAG>50</ANIM_LAG>      <ANIM_CODE>(A:AIRSPEED INDICATED, knots) 40 &lt; (L:Canopy Lever, bool) &amp;&amp; if{ 100 } els{ 0 }</ANIM_CODE>  </UseTemplate></Template><Template Name="DC_Burner_ANIM">  <UseTemplate Name="DCDesigns_GT_Animation">      <ANIM_NAME>Left_burner_coneAction</ANIM_NAME>	  <ANIM_LENGTH>100</ANIM_LENGTH>      <ANIM_CODE>(A:GENERAL ENG RPM:1,degrees per second) (E:ABSOLUTE TIME, second) * 360 % 3.6 /</ANIM_CODE>  </UseTemplate></Template><Template Name="DC_Burner_Right_ANIM">  <UseTemplate Name="DCDesigns_GT_Animation">      <ANIM_NAME>Right_burner_coneAction</ANIM_NAME>	  <ANIM_LENGTH>100</ANIM_LENGTH>      <ANIM_CODE>(A:GENERAL ENG RPM:2,degrees per second) (E:ABSOLUTE TIME, second) * 360 % 3.6 /</ANIM_CODE>  </UseTemplate></Template><Template Name="DC_Burner_Inner_ANIM">  <UseTemplate Name="DCDesigns_GT_Animation">      <ANIM_NAME>Afterburner_can_flamesAction</ANIM_NAME>	  <ANIM_LENGTH>100</ANIM_LENGTH>      <ANIM_CODE>(A:GENERAL ENG RPM:1,degrees per second) (E:ABSOLUTE TIME, second) * 360 % 3.6 /</ANIM_CODE>  </UseTemplate></Template><Template Name="DC_Burner_Inner_Right_ANIM">  <UseTemplate Name="DCDesigns_GT_Animation">      <ANIM_NAME>Afterburner_can_flames_rightAction</ANIM_NAME>	  <ANIM_LENGTH>100</ANIM_LENGTH>      <ANIM_CODE>(A:GENERAL ENG RPM:1,degrees per second) (E:ABSOLUTE TIME, second) * 360 % 3.6 /</ANIM_CODE>  </UseTemplate></Template><Template Name="DC_Exhaust_Petals">  <UseTemplate Name="DCDesigns_GT_Animation">      <ANIM_NAME>lever_throttle0</ANIM_NAME>	  <ANIM_LENGTH>100</ANIM_LENGTH>      <ANIM_CODE>(A:GENERAL ENG THROTTLE LEVER POSITION:1, percent) 95 &gt; if{ 0 } els{ 100 }</ANIM_CODE>      <ANIM_LAG>100</ANIM_LAG>	  </UseTemplate></Template><Template Name="DC_Exhaust_Petals_Right">  <UseTemplate Name="DCDesigns_GT_Animation">      <ANIM_NAME>lever_throttle1</ANIM_NAME>	  <ANIM_LENGTH>100</ANIM_LENGTH>      <ANIM_CODE>(A:GENERAL ENG THROTTLE LEVER POSITION:2, percent) 95 &gt; if{ 0 } els{ 100 }</ANIM_CODE>      <ANIM_LAG>100</ANIM_LAG>	  </UseTemplate></Template><Template Name="DC_Pilot_Head">  <UseTemplate Name="DCDesigns_GT_Animation">      <ANIM_NAME>Pilot_HeadAction</ANIM_NAME>	  <ANIM_LENGTH>100</ANIM_LENGTH>      <ANIM_CODE>(E:ABSOLUTE TIME, second) 6 % 25 *</ANIM_CODE>	  </UseTemplate></Template><Template Name="DC_WSO_Head">  <UseTemplate Name="DCDesigns_GT_Animation">      <ANIM_NAME>WSO_HeadAction</ANIM_NAME>	  <ANIM_LENGTH>100</ANIM_LENGTH>      <ANIM_CODE>(E:ABSOLUTE TIME, second) 8 % 25 *</ANIM_CODE>	  </UseTemplate></Template><Template Name="DC_Refuel_Door">  <UseTemplate Name="DCDesigns_GT_Animation">      <ANIM_NAME>RefuelDoorAction</ANIM_NAME>	  <ANIM_LENGTH>100</ANIM_LENGTH>      <ANIM_CODE>(L:Refuel Switch, bool) } if{ 100 } els{ 0 }</ANIM_CODE>      <ANIM_LAG>100</ANIM_LAG>	  </UseTemplate></Template><Template Name="DC_JFS_Starter">  <UseTemplate Name="DCDesigns_GT_Animation">      <ANIM_NAME>JFS_StarterAction</ANIM_NAME>	  <ANIM_LENGTH>50</ANIM_LENGTH>      <ANIM_CODE>(L:JFS_Starter, bool) } if{ 50 } els{ 0 }</ANIM_CODE>      <ANIM_LAG>100</ANIM_LAG>	  </UseTemplate></Template><Template Name="DC_Tailhook">  <UseTemplate Name="DCDesigns_GT_Animation">      <ANIM_NAME>Tailhook_HandleAction</ANIM_NAME>	  <ANIM_LENGTH>50</ANIM_LENGTH>      <ANIM_CODE>(L:Tailhook, bool) } if{ 50 } els{ 0 }</ANIM_CODE>      <ANIM_LAG>100</ANIM_LAG>	  </UseTemplate></Template><Template Name="DC_NAVGPS">  <UseTemplate Name="DCDesigns_GT_Animation">      <ANIM_NAME>GPS_NAV_PUSHAction</ANIM_NAME>	  <ANIM_LENGTH>50</ANIM_LENGTH>      <ANIM_CODE>(A:GPS DRIVES NAV1, bool) if{ 50 } els{ 0 }</ANIM_CODE>      <ANIM_LAG>100</ANIM_LAG>	  </UseTemplate></Template><Template Name="DC_Tailhook_External">  <UseTemplate Name="DCDesigns_GT_Animation">      <ANIM_NAME>HookCoverAction</ANIM_NAME>	  <ANIM_LENGTH>100</ANIM_LENGTH>      <ANIM_CODE>(L:Tailhook, bool ) if{ 100 } els{ 0 }</ANIM_CODE>      <ANIM_LAG>100</ANIM_LAG>	  </UseTemplate></Template>